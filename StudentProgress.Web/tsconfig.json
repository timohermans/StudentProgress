{
	"compilerOptions": {
		"types": ["vite/client"],
		"noImplicitAny": false,
		"noEmitOnError": true,
		"removeComments": false,
		"sourceMap": true,
		"target": "es6",
		"outDir": "wwwroot/js",
		"moduleResolution": "node"
	},
	"include": [
		"Client/**/*"
	],
	"exclude": [
		"node_modules",
		"wwwroot"
	]
}
